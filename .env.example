# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api/v1
VITE_API_TIMEOUT=10000
VITE_API_RETRIES=3

# API Features
VITE_ENABLE_API_LOGGING=true
VITE_ENABLE_RETRIES=true

# Request Deduplication (set to "true" to disable deduplication)
# Useful for testing or when you want to allow duplicate requests
VITE_DISABLE_REQUEST_DEDUPLICATION=false

# Optional Settings
# VITE_API_KEY=your-api-key-here

# Environment
VITE_APP_ENV=development
